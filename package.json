    "type": "git",
    "url": "git://github.com/opentable/injecorator.git"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-preset-node5": "^11.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-parameters": "^6.7.0",
    "babel-plugin-transform-es2015-spread": "^6.6.5",
    "chai": "^3.5.0",
    "eslint": "^2.2.0",
    "mocha": "^2.4.5",
    "nyc": "^6.4.0"
  },
  "dependencies": {}
}
