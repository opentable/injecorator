{
  "name": "injecorator",
  "version": "0.2.1",
  "description": "simple decorater based dependecy injection framework for nodejs es6 projects",
  "main": "./dist/injecorator.js",
  "scripts": {
    "coverage": "nyc --require babel-core/register mocha --recursive ./src/test/**",
    "lint": "eslint ./src",
    "build": "babel ./src -d dist",
    "test": "mocha --require babel-core/register ./src/test/**"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "author": "rickyaudsey@gmail.com",
  "contributors": [
    {
      "name": "Ricky Audsley",
      "email": "raudsley@opentable.com"
    },
    {
      "name": "Vasko Mitrevski",
      "email": "vmitrevski@opentable.com"
    }
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "IoC",
    "inversion of control",
    "di",
    "dependency injection",
    "injection",
    "injector",
    "container",
    "decorator",
    "decorators",
    "decorator pattern"
  ],
